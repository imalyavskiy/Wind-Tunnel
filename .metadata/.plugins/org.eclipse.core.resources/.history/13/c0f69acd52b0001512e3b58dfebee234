package org.demons.ctrl;

import org.zu.ardulink.Link;

public class WindSpeedDisplay {
	private Link link = Link.getDefaultInstance();
	
	private int[][] digitPin;
	
	public WindSpeedDisplay() {
		digitPin = new int[4][4];
		
		digitPin[0][0] = 28;
		digitPin[0][1] = 22;
		digitPin[0][2] = 24;
		digitPin[0][3] = 26;
		
		digitPin[1][0] = 36;
		digitPin[1][1] = 30;
		digitPin[1][2] = 32;
		digitPin[1][3] = 34;
		
		digitPin[2][0] = 29;
		digitPin[2][1] = 23;
		digitPin[2][2] = 25;
		digitPin[2][3] = 27;
		
		digitPin[3][0] = 37;
		digitPin[3][1] = 31;
		digitPin[3][2] = 33;
		digitPin[3][3] = 35;
	}
}
